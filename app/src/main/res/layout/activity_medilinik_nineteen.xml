                        android:layout_height="@dimen/_32pxh"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_gray_50_32x36"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_50_32x36"/>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:gravity="center_horizontal"
                    android:id="@+id/recyclerListduration"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_28pxh"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_29pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_42pxh}" tools:itemCount="2" tools:listitem="@layout/row_listduration"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnAddReminder"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_28pxh"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_9pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_add_reminder"
                    android:textAllCaps="false"
                    style="@style/btnGradientRounded" tools:text="@string/lbl_add_reminder"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageRectangleFive"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_12pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_rectangle5_12x411"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle5_12x411"/>
        </FrameLayout>
        <LinearLayout android:id="@+id/linearHome"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylewhite_12">
            <ImageView android:id="@+id/imageVectorOne"
                android:layout_height="@dimen/_21pxh"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginStart="@dimen/_50pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="@dimen/_17pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_gray_400"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_400"/>
            <ImageView android:id="@+id/imageVectorTwo"
                android:layout_height="@dimen/_21pxh"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginStart="@dimen/_55pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="@dimen/_17pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_gray_400_19x16"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_400_19x16"/>
            <FrameLayout android:id="@+id/frameStackvectorThree"
                android:layout_height="@dimen/_19pxv"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginStart="@dimen/_55pxh"
                android:layout_marginTop="@dimen/_7pxv" android:layout_width="@dimen/_19pxh">
                <ImageView android:id="@+id/imageVectorThree"
                    android:layout_gravity="top|center"
                    android:layout_height="@dimen/_9pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_2pxh"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_gray_400_9x14"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_400_9x14"/>
                <ImageView android:id="@+id/imageVectorFour"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_12pxv"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_gray_400_14x19"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_400_14x19"/>
            </FrameLayout>
            <ImageView android:id="@+id/imageVectorFive"
                android:layout_height="@dimen/_19pxh"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginStart="@dimen/_56pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_gray_400_19x19"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_400_19x19"/>
            <ImageView android:id="@+id/imageVectorSix"
                android:layout_height="@dimen/_18pxh"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginEnd="@dimen/_42pxh"
                android:layout_marginStart="@dimen/_57pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_17pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_gray_400_17x17"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_400_17x17"/>
        </LinearLayout>
    </LinearLayout>
</layout>
