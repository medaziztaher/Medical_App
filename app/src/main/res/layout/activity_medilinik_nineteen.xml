<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="medilinikNineteenVM" type="com.mohamednethirsapplication.app.modules.mediliniknineteen.data.viewmodel.MedilinikNineteenVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearMedilinikNinet"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/frameStacktimeschedule"
            android:layout_height="@dimen/_642pxv" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearColumnmedicine531037"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:gravity="start"
                    android:id="@+id/frameStackmedicine531037"
                    android:layout_height="@dimen/_255pxv"
                    android:layout_width="match_parent" style="@style/groupStylecornerRadius_4">
                    <ImageView android:id="@+id/imageMedicine531037"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_225pxh"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_marginEnd="@dimen/_40pxh"
                        android:layout_marginStart="@dimen/_40pxh"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:layout_width="@dimen/_225pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_medicine531037}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_medicine531037"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameStackmedecinename"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_81pxv"
                    android:layout_marginEnd="@dimen/_28pxh"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_13pxv" android:layout_width="@dimen/_340pxh">
                    <TextView android:ellipsize="end"
                        android:gravity="top|start"
                        android:id="@+id/txtMedecineName"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{medilinikNineteenVM.medilinikNineteenModel.txtMedecineName}"
                        style="@style/txtPoppinsbold25" tools:text="@string/lbl_medecine_name"/>
                    <View android:background="@color/bluegray_100"
                        android:id="@+id/lineVectorNine"
                        android:layout_gravity="bottom|center"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_336pxh"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAmoxicillin"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{medilinikNineteenVM.medilinikNineteenModel.txtAmoxicillin}"
                        style="@style/txtPoppinssemibold30" tools:text="@string/lbl_amoxicillin"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtTimeSchedule"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_35pxh"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{medilinikNineteenVM.medilinikNineteenModel.txtTimeSchedule}"
                    style="@style/txtPoppinsbold25" tools:text="@string/lbl_time_schedule"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowafterBreakfastOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_28pxh"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnAfterBreakfastOne"
                        android:layout_height="@dimen/_49pxv"
                        android:layout_width="@dimen/_130pxh"
                        android:paddingBottom="@dimen/_14pxv"
                        android:paddingEnd="@dimen/_13pxh"
                        android:paddingStart="@dimen/_13pxh"
                        android:paddingTop="@dimen/_14pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_after_breakfast"
                        android:textAllCaps="false"
                        style="@style/btnSolidRoundedOutline_1" tools:text="@string/lbl_after_breakfast"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnAfterDinner"
                        android:layout_height="@dimen/_49pxv"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_width="@dimen/_130pxh"
                        android:paddingBottom="@dimen/_14pxv"
                        android:paddingEnd="@dimen/_23pxh"
                        android:paddingStart="@dimen/_23pxh"
                        android:paddingTop="@dimen/_14pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_after_dinner"
                        android:textAllCaps="false"
                        style="@style/btnSolidRoundedOutline_2" tools:text="@string/lbl_after_dinner"/>
