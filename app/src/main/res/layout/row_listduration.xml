<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listdurationRowModel" type="com.mohamednethirsapplication.app.modules.mediliniknineteen.data.model.ListdurationRowModel"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnduration"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" android:orientation="vertical">
        <TextView android:ellipsize="end" android:id="@+id/txtDuration"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{listdurationRowModel.txtDuration}"
            style="@style/txtPoppinsbold25" tools:text="@string/lbl_duration"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowcalendar277328"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_3pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageCalendar277328"
                android:layout_height="@dimen/_37pxh"
                android:layout_width="@dimen/_44pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_calendar277328"
                tools:ignore="ContentDescription" tools:src="@drawable/img_calendar277328"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDurationOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_weight="0.62" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{listdurationRowModel.txtDurationOne}"
                style="@style/txtPoppinsbold18_1" tools:text="@string/lbl_1_month"/>
        </LinearLayout>
        <View android:background="@color/bluegray_100"
            android:id="@+id/lineVectorTwelve"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_3pxh"
            android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_158pxh"/>
    </LinearLayout>
</layout>
